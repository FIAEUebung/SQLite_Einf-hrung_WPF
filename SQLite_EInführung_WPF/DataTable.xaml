<Window x:Class="SQLite_EInführung_WPF.DataTable"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SQLite_EInführung_WPF"
        mc:Ignorable="d"
        Title="DataTable" Height="450" Width="800">
    <Grid Background="DarkRed">
        <StackPanel Margin="0,80,0,80" Background="#FF838383">
            <DataGrid x:Name="Data_Table_DataGrid" Height="200" Width="600" Background="Transparent" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,30,0,29"/>
        </StackPanel>
    </Grid>
</Window>
